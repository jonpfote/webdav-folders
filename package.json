{
  "name": "webdav",
  "displayName": "%displayName%",
  "description": "%description%",
  "publisher": "jonpfote",
  "icon": "webdav-logo.png",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "Other"
  ],
  "capabilities": {
    "untrustedWorkspaces": {
      "supported": true
    }
  },
  "activationEvents": [
